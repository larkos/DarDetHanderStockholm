    <link href="http://fonts.googleapis.com/css?family=Kaushan+Script" rel="stylesheet" type="text/css">*@
    <script>
      
    </script>

</head>

<body>
  @for (int i = 0; i < Model.Count(); i++)
  {
      <div class="Advertise">
          <div class="polaroidArea">
              <div class="polaroid">
                  <a href="@Model[i].EventUrl"><img class="pimpedevents" src="@Model[i].ImageUrl" width="250" height="250"></a>
                  <p><a href="@Model[i].EventUrl" style="color:firebrick;font-size:medium">@Model[i].EventDate</a></p>
                      
                  
              </div>
              
          </div>
          <div class="textarea">
         
              @Model[i].EventName
          
              @Html.Raw(@Model[i].LongDescription)
          </div>
          
      </div>
    
      
      
  }
</body>
</html>
